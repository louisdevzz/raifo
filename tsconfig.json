{
  "compilerOptions": {
    "paths": {
      "@/*":["./*"]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "utils/config.jsx", "database/conferencePaper.ts", "database/titleResearch.ts", "database/scientificArticle.ts", "database/event.jsx", "components/About/index.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
